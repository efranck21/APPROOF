
set log
set xlabel 'N'
set ylabel 'L2 Error' 

plot 'AP_random_gamma2' using 1:3 with lp pt 2  title "2:",'AP_random_gamma1' using 1:3 with lp pt 3  title "1:" ,'AP_random_gamma12' using 1:3 with lp pt 4  title "0.5:",'AP_random_gamma14' using 1:3 with lp pt 5 title "0.25:",'AP_random_gamma0' using 1:3 with lp pt 6 title "0:", .5/x with l title 'order one'

set term postscript   monoc solid 
set output "sortie.eps"
replot 
